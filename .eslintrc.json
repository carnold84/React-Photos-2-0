{
  "extends": [
    "react-app"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "indent": [
      "error",
      2,
      {
        "outerIIFEBody": 2
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ]
  }
}